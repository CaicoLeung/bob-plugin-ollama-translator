  "scripts": {
    "build": "npx tsx scripts/build.ts",
    "build:bun": "bun scripts/build.ts",
    "build:node22": "NODE_OPTIONS=\"--experimental-strip-types\" zx scripts/build.ts"
  },
  "dependencies": {
    "@bob-translate/types": "^1.0.2",
    "@rollup/plugin-terser": "0.4.4",
