    "bun": "^1.2.11",
    "zx": "^8.5.3"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "eventsource-parser": "^3.0.1",
    "globals": "^16.0.0",
    "openai": "^4.96.2",
    "prettier": "^3.5.3",
    "rollup": "^4.40.1",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-delete": "^3.0.1",
    "tslib": "2.8.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.1"
  },
  "packageManager": "pnpm@10.10.0"
}